<%= render('layouts/errors', :object => @project) %>

<%= form_for [@student, @project] do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :url %>
  <%= f.text_field :url %>
  <p><%= f.submit %></p>
<% end %>
